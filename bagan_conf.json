{
   "bagan": {
      "repository": "https://github.com/android/plaid.git",
      "gradleCommand": "./gradlew clean assembleDebug",
      "clusterName": "bagan",
      "machine": "n2-standard-8",
      "zone": "europe-west4-b",
      "private": false,
      "ssh": "",
      "known_hosts": "",
      "iterations": 20,
      "experiments": {
         "properties": [
            {
               "name": "org.gradle.jvmargs",
               "options": ["-Xmx4g","-Xmx6g"]
            },
            {
               "name": "org.gradle.workers.max",
               "options": ["1","3"]
            }
         ]
      }
   }
}
